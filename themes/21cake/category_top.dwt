    <h1 class="menlogo Womenlogo"><a href="{$cat_info.url}" class="track">{$cat_info.cat_name}</a></h1>
    <div class="menlist">
      <ul>
        <!--{foreach from=$categories item=cat}--> 
        <!--{if $cat.id eq $category}--> 
        <!--{foreach from=$cat.cat_id item=child name=foo}--> 
        <!--{if $smarty.foreach.foo.iteration < 10}-->
        <li><a href="{$child.url}" class="track">{$child.name|escape:html}</a></li>
        <!--{/if}--> 
        <!--{/foreach}--> 
        <!--{/if}--> 
        <!-- {/foreach} -->
      </ul>
    </div>
  </div>
</div>
